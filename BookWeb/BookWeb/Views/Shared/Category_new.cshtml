@model List<Model.Entity.Category>

<div class="row">
    <div class="col-lg-12">
        <div class="section-title">
            <h2>San Pham Moi</h2>
        </div>
        <div class="featured__controls">
            <ul>
                @foreach (var item in Model)
                {
                    <li data-filter="@item.id">@item.name</li>
                }
            </ul>
        </div>
    </div>
</div>
